{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "etl-server": {
      "projectType": "library",
      "root": "projects/etl-server",
      "sourceRoot": "projects/etl-server/src",
      "prefix": "etl",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/etl-server/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/etl-server/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/etl-server/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/etl-server/src/test.ts",
            "tsConfig": "projects/etl-server/tsconfig.spec.json",
            "karmaConfig": "projects/etl-server/karma.conf.js"
          }
        }
      }
    },
    "ui": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "less"
        }
      },
      "root": "projects/ui",
      "sourceRoot": "projects/ui/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ui",
            "index": "projects/ui/src/index.html",
            "main": "projects/ui/src/main.ts",
            "polyfills": "projects/ui/src/polyfills.ts",
            "tsConfig": "projects/ui/tsconfig.app.json",
            "inlineStyleLanguage": "less",
            "assets": [
              "projects/ui/src/favicon.ico",
              "projects/ui/src/assets"
            ],
            "styles": [
              "projects/ui/src/styles.less"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/ui/src/environments/environment.ts",
                  "with": "projects/ui/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "localize": ["en", "he"]
            },
            "wpdx": {
              "fileReplacements": [
                {
                  "replace": "projects/ui/src/environments/environment.ts",
                  "with": "projects/ui/src/environments/environment.wpdx.ts"
                }
              ],
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ui:build:production"
            },
            "wpdx": {
              "browserTarget": "ui:build:wpdx"
            },
            "development": {
              "browserTarget": "ui:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ui:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ui/src/test.ts",
            "polyfills": "projects/ui/src/polyfills.ts",
            "tsConfig": "projects/ui/tsconfig.spec.json",
            "karmaConfig": "projects/ui/karma.conf.js",
            "inlineStyleLanguage": "less",
            "assets": [
              "projects/ui/src/favicon.ico",
              "projects/ui/src/assets"
            ],
            "styles": [
              "projects/ui/src/styles.less"
            ],
            "scripts": []
          }
        }
      },
      "i18n": {
        "sourceLocale": {
          "code": "en",
          "baseHref": "/en/"
        },
        "locales": {
          "he": {
            "translation": "projects/ui/src/locales/messages.he.xlf",
            "baseHref": "/he/"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
