{
  "name": "dgp-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n": "ng xi18n --ivy --output-path src/locales && tx push --source && tx pull --mode sourceastranslation -l he,en",
    "package": "ng build --prod --localize",
    "build_lib": "ng build etl-server --prod",
    "watch_lib": "ng build etl-server --watch",
    "copy-license": "cp ./LICENSE ./dist/etl-server",
    "copy-readme": "cp ./README.md ./dist/etl-server",
    "copy-locales": "cp -r ./src/locales ./dist/etl-server",
    "copy-files": "npm run copy-license && npm run copy-readme && npm run copy-locales",
    "npm_pack": "cd dist/etl-server && npm pack",
    "package_lib": "npm run build_lib && npm run copy-files",
    "publish_lib": "npm run package_lib && cd dist/etl-server && npm publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.16",
    "@angular/common": "~12.2.16",
    "@angular/compiler": "~12.2.16",
    "@angular/core": "~12.2.16",
    "@angular/forms": "~12.2.16",
    "@angular/platform-browser": "~12.2.16",
    "@angular/platform-browser-dynamic": "~12.2.16",
    "@angular/router": "~12.2.16",
    "core-js": "^3.6.5",
    "moment": "^2.29.1",
    "ng-event-source": "^1.0.14",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.1",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.18",
    "@angular/cli": "~12.2.18",
    "@angular/compiler-cli": "~12.2.16",
    "@angular/language-service": "~12.2.16",
    "@angular/localize": "^12.2.16",
    "@types/jasmine": "~3.5.14",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~14.11.2",
    "codelyzer": "~6.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^12.2.7",
    "protractor": "~7.0.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "typescript": "~4.3.5"
  }
}